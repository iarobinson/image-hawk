<p id="notice"><%= notice %></p>
<div class="container">
  <div class="row">
    <div class="<%= common_column_spacing %>">
      <h1>All Sessions</h1>

      <% @sessions.each do |session| %>
        <%= render session %>
      <% end %>

      <%= link_to 'New Session', new_session_path, class: "btn btn-primary" %>
      <%= link_to 'Dashboard', root_path, class: "btn btn-success" %>
    </div>
  </div>
</div>